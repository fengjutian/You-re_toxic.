<view class="container">
  <view class="section">
    <view class="title">关系整体状态</view>
    <view class="text">{{analysis.status}}</view>
  </view>

  <view class="section">
    <view class="title">他人行为风险维度</view>
    <view wx:for="{{analysis.tirsHigh}}" wx:key="item" class="dimension-item">{{item}}</view>
  </view>

  <view class="section">
    <view class="title">主要自我消耗体验</view>
    <view class="text">{{analysis.rtdrsTop}}</view>
  </view>

  <button class="next-btn" bindtap="goTips">查看自我保护建议</button>
</view>
